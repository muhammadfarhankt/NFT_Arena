<!DOCTYPE html>
<html lang="en">

<head>
	<meta charset="UTF-8">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">

	<!-- Boxicons -->
	<link href='https://unpkg.com/boxicons@2.0.9/css/boxicons.min.css' rel='stylesheet'>
	<!-- My CSS -->
	<link rel="stylesheet" href="/adminhome/style.css">

	<title>Dashboard - NFT Arena</title>
</head>
<style>
	.order-card {
		color: #fff;
	}

	.bg-c-blue {
		background: linear-gradient(45deg, #4099ff, #73b4ff);
	}

	.bg-c-green {
		background: linear-gradient(45deg, #2ed8b6, #59e0c5);
	}

	.bg-c-yellow {
		background: linear-gradient(45deg, #FFB64D, #ffcb80);
	}

	.bg-c-pink {
		background: linear-gradient(45deg, #FF5370, #ff869a);
	}


	.card {
		border-radius: 5px;
		-webkit-box-shadow: 0 1px 2.94px 0.06px rgba(4, 26, 55, 0.16);
		box-shadow: 0 1px 2.94px 0.06px rgba(4, 26, 55, 0.16);
		border: none;
		margin-bottom: 30px;
		-webkit-transition: all 0.3s ease-in-out;
		transition: all 0.3s ease-in-out;
	}

	.card .card-block {
		padding: 25px;
	}

	.order-card i {
		font-size: 26px;
	}

	.f-left {
		float: left;
	}

	.f-right {
		float: right;
	}
</style>

<body>

	<!-- SIDEBAR -->
	<section id="sidebar">
		<a href="dashboard" class="brand">
			<i class='bx bxs-smile'></i>
			<span class="text">NFT Arena</span>
		</a>
		<ul class="side-menu top">
			<li class="active">
				<a href="dashboard">
					<i class='bx bxs-dashboard'></i>
					<span class="text">Dashboard</span>
				</a>
			</li>
			<li>
				<a href="products">
					<i class='bx bxl-product-hunt'></i>
					<span class="text">Products</span>
				</a>
			</li>
			<li>
				<a href="category">
					<i class='bx bxs-category-alt'></i>
					<span class="text">Categories</span>
				</a>
			</li>
			<li>
				<a href="author">
					<i class='bx bxs-message-dots'></i>
					<span class="text">Authors</span>
				</a>
			</li>
			<li>
				<a href="user">
					<i class='bx bxs-group'></i>
					<span class="text">Users</span>
				</a>
			</li>
			<li>
				<a href="banners">
					<i class='bx bxs-carousel'></i>
					<span class="text">Banners</span>
				</a>
			</li>
			<li>
				<a href="orders">
					<i class='bx bxs-shopping-bag-alt'></i>
					<span class="text">Orders</span>
				</a>
			</li>
			<li>
				<a href="">
					<i class='bx bxs-coupon'></i>
					<span class="text" style="text-decoration: line-through;">Coupons</span>
				</a>
			</li>

		</ul>
		<ul class="side-menu">
			<li>
				<a href="">
					<i class='bx bxs-cog'></i>
					<span class="text" style="text-decoration: line-through;">Settings</span>
				</a>
			</li>
			<li>
				<a href="/admin/adminlogout" class="logout">
					<i class='bx bxs-log-out-circle'></i>
					<span class="text">Logout</span>
				</a>
			</li>
		</ul>
	</section>
	<!-- SIDEBAR -->



	<!-- CONTENT -->
	<section id="content">
		<!-- NAVBAR -->
		<nav>
			<i class='bx bx-menu'></i>
			<a href="#" class="nav-link">Search whole data</a>
			<form action="#">
				<div class="form-input">
					<input type="search" placeholder="Search...">
					<button type="submit" class="search-btn"><i class='bx bx-search'></i></button>
				</div>
			</form>
			<input type="checkbox" id="switch-mode" hidden>
			<label for="switch-mode" class="switch-mode"></label>
			<a href="#" class="notification">
				<i class='bx bxs-bell'></i>
				<span class="num">8</span>
			</a>
			<a href="#" class="profile">
				<img
					src="data:image/png;base64,">
			</a>
		</nav>
		<!-- NAVBAR -->

		<!-- MAIN -->
		<main>
			<div class="head-title">
				<div class="left">
					<h1>Admin Dashboard</h1>
					<ul class="breadcrumb">
						<li>
							<a href="">Dashboard</a>
						</li>
						<li><i class='bx bx-chevron-right'></i></li>
						<li>
							<a class="active" href="">Home</a>
						</li>
					</ul>
				</div>
				<a href="/admin/salesReport" class="btn-download">
					<i class='bx bxs-cloud-download'></i>
					<span class="text">Download Sales Report</span>
				</a>
			</div>

			<ul class="box-info">
				<li>
					<i class='bx bxs-group'></i>
					<span class="text">
						<h3>
							<%= userCount %>
						</h3>
						<p>Users</p>
					</span>
				</li>

				<li>
					<i class='bx bxs-shopping-bag-alt'></i>
					<span class="text">
						<h3>
							<%= orderCount %>
						</h3>
						<p>Orders</p>
					</span>
				</li>


				<li>
					<i class='bx bxs-group'></i>
					<span class="text">
						<h3>
							<%= authorCount %>
						</h3>
						<p>Authors</p>
					</span>
				</li>
				<li>
					<i class='bx bxl-product-hunt'></i>
					<span class="text">
						<h3>
							<%= productCount %>
						</h3>
						<p>Products</p>
					</span>
				</li>

				<li>
					<i class='bx bxs-bar-chart-alt-2'></i>
					<span class="text">
						<h3>
							<%= categoryCount %>
						</h3>
						<p>Categories</p>
					</span>
				</li>

				<li>
					<i class='bx bxs-carousel'></i>
					<span class="text">
						<h3>
							<%= bannerCount %>
						</h3>
						<p>Banners</p>
					</span>
				</li>

				<li>
					<i class='bx bxs-coupon'></i>
					<span class="text">
						<h3>
							<% couponCount %>
								0
						</h3>
						<p style="text-decoration:line-through;">Coupons</p>
					</span>
				</li>

				<li>
					<i class='bx bxs-dollar-circle'></i>
					<span class="text">
						<h3>$ <%= total %>
						</h3>
						<p>Total Sales</p>
					</span>
				</li>

			</ul>


			<div class="table-data">
				<div class="order">
					<div class="head">
						<h3>Recent Orders</h3>
					</div>
					<canvas id="myChart"></canvas>


					<script src="https://cdn.jsdelivr.net/npm/chart.js"></script>
					<input type="hidden" value="<%=amount %>" id="amount" />

					<script>
						const ctx = document.getElementById("myChart");
						const a = document.getElementById("amount").value
						const k = a.split(',').map(c => {
							return parseInt(c)
						})
						console.log(k);
						new Chart(ctx, {
							type: "bar",
							data: {
								labels: [
									"Sunday",
									"Monday",
									"Tuesday",
									"Wednesday",
									"Thursday",
									"Friday",
									"Saturday",
								],
								datasets: [
									{
										label: "amount",
										data: k,
										borderWidth: 1,
									},
								],
							},
							options: {
								scales: {
									y: {
										beginAtZero: true,
									},
								},
							},
						});
					</script>
				</div>
				<div class="todo">
					<input type="hidden" value="<%=totalSales%>" id="circularChart">
					<input type="hidden" value="<%=categoryNames%>" id="circularChartNames">
					<div class="head">
						<h3>Category Wise Sales</h3>
						
					</div>
					<canvas id="myChart1" style="display: block;"></canvas>
				</div>
				<div class="todo">
					<div class="head">
						<h3>Author Wise Sales</h3>
						<input type="hidden" value="<%=totalSalesAuthor%>" id="circularChart2">
						<input type="hidden" value="<%=authorNames%>" id="circularChartNames2">
					</div>
					<canvas id="myChart2" style="display: block;"></canvas>
				</div>
				
			</div>
		</main>
		<!-- MAIN -->
	</section>
	<!-- CONTENT -->


	<script src="/adminhome/script.js"></script>
	<script>
		const category = document.getElementById("circularChart").value.split(',').map(Number)
		const categoryNames = document.getElementById("circularChartNames").value.split(',').map(String)
		const author = document.getElementById("circularChart2").value.split(',').map(Number)
		const authorNames = document.getElementById("circularChartNames2").value.split(',').map(String)
		console.log(categoryNames)
		// Set up data for the pie chart
		const data = {
			labels: categoryNames,
			datasets: [{
				label: 'Sales by Category',
				data: category,
				backgroundColor: [
					'rgba(255, 99, 132, 0.5)',
					'rgba(54, 162, 235, 0.5)',
					'rgba(255, 206, 86, 0.5)'
				],
				borderColor: [
					'rgba(255, 99, 132, 1)',
					'rgba(54, 162, 235, 1)',
					'rgba(255, 206, 86, 1)'
				],
				borderWidth: 1
			}]
		};
		const data2 = {
			labels: authorNames,
			datasets: [{
				label: 'Sales by Author',
				data: author,
				backgroundColor: [
					'rgba(255, 99, 132, 0.5)',
					'rgba(54, 162, 235, 0.5)',
					'rgba(255, 206, 86, 0.5)'
				],
				borderColor: [
					'rgba(255, 99, 132, 1)',
					'rgba(54, 162, 235, 1)',
					'rgba(255, 206, 86, 1)'
				],
				borderWidth: 1
			}]
		};
		// Create the pie chart
		const ctx1 = document.getElementById('myChart1').getContext('2d');
		const myChart = new Chart(ctx1, {
			type: 'pie',
			data: data,
			options: {}
		});
		// Create the pie chart 2
		const ctx2 = document.getElementById('myChart2').getContext('2d');
		const myChart2 = new Chart(ctx2, {
			type: 'pie',
			data: data2,
			options: {}
		});
	</script>
	<script type="text/javascript" src="https://www.gstatic.com/charts/loader.js"></script>

</body>

</html>